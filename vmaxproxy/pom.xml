<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <!--
    Example pom for delegating maven goals to msbuild so it builds the "solution"
 
    A solution can be a group of projects, but here we only have one.
    It should also be possible to have one maven project per solution project, 
    and have a parent pom to deletegate the build.
  -->
	<parent>
		<artifactId>cpp</artifactId>
		<groupId>com.networkoptix.hdwitness</groupId>
		<version>2.3.0-SNAPSHOT</version>
		<relativePath>../cpp</relativePath>
	</parent>

	<groupId>com.networkoptix.hdwitness</groupId>
	<version>2.3.0-SNAPSHOT</version>
	<modelVersion>4.0.0</modelVersion>
	<artifactId>vmaxproxy</artifactId>
	<packaging>pom</packaging>
	<name>HD Witness VMax Proxy</name>	

    <properties>
        <!--This module is forced to be built under x86. This is done by "replace.skip" property and ./maven/vmaxproxy-specifics.pro-->
        <!--Skipping replace plugin. It replaces Win32->x64 in vcproj files for x64 build.-->
        <product.title>${vmaxproxy.name}</product.title>	
        <force_x86>true</force_x86>
        <!--Global libs-->
        <qt.libs>core</qt.libs>
        <global.libs/>
        <!--Specific os libs used-->
        <windows.oslibs>-lws2_32 -lIphlpapi -lOle32 -lwinmm -lacs_codec_mr -lacs_event_md -lacs_event_mr -lacs_export_mr -lacs_post_processing_mr -lacs_stream_source_mr</windows.oslibs>
        <windows.oslibs.release></windows.oslibs.release> 
        <windows.oslibs.debug></windows.oslibs.debug>
        <linux.oslibs/>
        <mac.oslibs/>
        <!--Global defines-->
        <global.defines/>
        <!--Specific os defines-->
        <windows.defines/>
        <windows.defines/>
        <linux.defines/>
        <mac.defines/>	
    </properties>  
</project>
