	<groupId>com.networkoptix.hdwitness</groupId>
	<version>4.0-SNAPSHOT</version>
	<modelVersion>4.0.0</modelVersion>
	<artifactId>vmaxproxy</artifactId>
	<packaging>pom</packaging>
	<name>Nx Witness VMax Proxy</name>	

    <properties>
        <!--This module is forced to be built under x86. This is done by "replace.skip" property and ./maven/vmaxproxy-specifics.pro-->
        <!--Skipping replace plugin. It replaces Win32->x64 in vcproj files for x64 build.-->
        <product.title>${vmaxproxy.name}</product.title>	
        <force_x86>true</force_x86>
        <template>app</template>

        <qt.libs>core</qt.libs>
        <windows.oslibs>-lws2_32 -lIphlpapi -lOle32 -lwinmm -lacs_codec_mr -lacs_event_md -lacs_event_mr -lacs_export_mr -lacs_post_processing_mr -lacs_stream_source_mr</windows.oslibs>
    </properties>  
</project>
