<?xml version='1.0' encoding='windows-1252'?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
    <Fragment Id='FragmentNxtoolPackage'>
        <PackageGroup Id="NxtoolPackageGroup">
            <MsiPackage
                Id="NxtoolPackage"
                Name="$(var.NxtoolMsiName)"
                SourceFile="$(var.NxToolMsiFile)"
                Vital="yes"
                Compressed="yes">
            </MsiPackage>
        </PackageGroup>
    </Fragment>
</Wix>
