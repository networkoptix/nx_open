{
    "systems": {
        "info": {
            "values": {
                "name": {
                    "display": "panel"
                },
                "recommendedMaxServers": {
                    "calculate": "const 100"
                },
                "servers": {
                    "display": "panel",
                    "alarms": [{
                        "level": "warning",
                        "condition": "greaterThan %servers %recommendedMaxServers",
                        "text": "The maximum number of %recommendedMaxServers servers per system is reached. Create another system to use more servers"
                    }]
                },
                "users": {
                    "display": "panel"
                },
                "storages": {
                    "name": "Storage locations",
                    "display": "panel"
                },
                "recommendedMaxCameras": {
                    "calculate": "const 10000"
                },
                "cameras": {
                    "name": "Camera channels",
                    "display": "panel",
                    "alarms": [{
                        "level": "warning",
                        "condition": "greaterThan %servers %recommendedMaxCameras",
                        "text": "The maximum number of %recommendedMaxCameras camera channels per system is reached. Create another system to use more cameras"
                    }]
                },
                "version": {
                    "display": "panel"
                }
            }
        }
    },
    "servers": {
        "info": {
            "values": {
                "name": {
                    "name": "Name",
                    "display": "table|panel"
                },
                "cpu": {
                    "name": "CPU",
                    "display": "panel"
                },
                "ram": {
                    "name": "RAM",
                    "format": "GB",
                    "display": "panel"
                },
                "os": {
                    "name": "OS",
                    "display": "panel"
                },
                "publicIp": {
                    "name": "Public IP",
                    "display": "table|panel"
                },
                "vmsTime": {
                    "name": "VMS time",
                    "display": "table|panel"
                },
                "osTime": {
                    "name": "OS time",
                    "display": "table|panel"
                },
                "cores": {
                    "display": "panel"
                },
                "timeChanged24h": {
                    "name": "Time changedevents (24h)",
                    "display": "table|panel",
                    "calculate": "sum %timeChanged 24h",
                    "insert": "timeChanged",
                    "alarms": [
                        {
                            "level": "warning",
                            "condition": "greaterThan %timeChanged24h 5",
                            "text": "has changed time %timeChanged24h times in last 24 hours"
                        }
                    ]
                }
            }
        },
        "availability": {
            "values": {
                "status": {
                    "display": "table|panel",
                    "alarms": [{
                        "level": "error",
                        "condition": "notEqual %status Online",
                        "text": "Status is %status"
                    }]
                },
                "offlineEvents": {
                    "name": "Server Offline events (24h)",
                    "display": "table|panel",
                    "calculate": "countValues %status 24h Offline",
                    "insert": "uptimeS",
                    "alarms": [{
                        "level": "warning",
                        "condition": "greaterThan %offlineEvents 1",
                        "text": "went Offline %offlineEvents times in last 24 hours"
                    }]
                },
                "uptimeS": {
                    "name": "Uptime",
                    "format": "durationS",
                    "display": "table|panel"
                }
            }
        },
        "serverLoad": {
            "values": {
                "cpuUsageP": {
                    "name": "CPU %",
                    "format": "%",
                    "display": "table|panel",
                    "alarms": [{
                        "level": "warning",
                        "condition": "greaterThan %cpuUsageP 0.9",
                        "text": "Total CPU Usage is %cpuUsageP. VMS is using %serverCpuUsageP."
                    }]
                },
                "serverCpuUsageP": {
                    "name": "CPU (VMS Server) %",
                    "format": "%",
                    "display": "table|panel"
                },
                "ramUsageB": {
                    "name": "RAM Usage",
                    "format": "GB",
                    "display": "panel"
                },
                "ramUsageP": {
                    "name": "RAM %",
                    "format": "%",
                    "display": "table|panel",
                    "alarms": [{
                        "level": "warning",
                        "condition": "greaterThan %ramUsageP 0.8",
                        "text": "Total RAM usage is %ramUsageP. VMS is using %serverRamUsageP."
                    }]
                },
                "serverRamUsage": {
                    "name": "RAM (VMS Server)",
                    "format": "GB",
                    "display": "panel"
                },
                "serverRamUsageP": {
                    "name": "RAM (VMS Server) %",
                    "format": "%",
                    "display": "table|panel"
                },
                "incomingConnections": {
                    "name": "Incoming connections",
                    "display": "panel"
                },
                "encodingThreads": {
                    "name": "Encoding threads",
                    "display": "panel",
                    "alarms": [
                        {
                            "level": "warning",
                            "condition": "greaterThan %encodingThreads 2",
                            "text": "Encoding threads %encodingThreads could cause high server load"
                        }
                    ]
                },
                "decodingThreads": {
                    "name": "Decoding threads",
                    "display": "panel"
                },
                "cameras": {
                    "name": "Cameras",
                    "display": "panel"
                },
                "transactionsPerSecond1m": {
                    "name": "Transactions per second",
                    "display": "panel",
                    "format": "Transactions/s",
                    "calculate": "average %transactionsPerSecond 1m",
                    "insert": "transactionsPerSecond"
                },
                "decodingSpeed3s": {
                    "name": "Decoding speed",
                    "display": "panel",
                    "format": "Mp/s",
                    "calculate": "average %decodedPixels 3s",
                    "insert": "decodedPixels"
                },
                "encodingSpeed3s": {
                    "name": "Encoding speed",
                    "display": "panel",
                    "format": "Mp/s",
                    "calculate": "average %encodedPixels 3s",
                    "insert": "encodedPixels"
                },
                "actionsTriggered1m": {
                    "name": "Event rules triggering/s",
                    "display": "panel",
                    "calculate": "average %actionsTriggered 1m",
                    "insert": "actionsTriggered"
                },
                "thumbnails1m": {
                    "name": "Thumbnails per second",
                    "display": "panel",
                    "calculate": "average %thumbnails 1m",
                    "insert": "thumbnails"
                },
                "apiCalls1m": {
                    "name": "Rest API calls per second",
                    "display": "panel",
                    "calculate": "average %apiCalls 1m",
                    "insert": "apiCalls"
                },
                "primaryStreams": {
                    "name": "Primary streams",
                    "display": "panel"
                },
                "secondaryStreams": {
                    "name": "Secondary streams",
                    "display": "panel"
                }
            }
        }
    },
    "cameras": {
        "info": {
            "values": {
                "name": {
                    "display": "table|panel"
                },
                "server": {
                    "display": "table|panel"
                },
                "type": {
                    "display": "table|panel"
                },
                "ip": {
                    "name": "IP",
                    "display": "table|panel"
                },
                "vendor": {
                    "display": "panel"
                },
                "model": {
                    "display": "panel"
                },
                "firmware": {
                    "display": "panel"
                },
                "recording": {
                    "name": "Recording",
                    "display": "table|panel"
                }
            }
        },
        "availability": {
            "values": {
                "status": {
                    "display": "table|panel",
                    "alarms": [
                        {
                            "level": "warning",
                            "condition": "equal %status Unauthorized",
                            "text": "Status is %status"
                        },
                        {
                            "level": "error",
                            "condition": "equal %status Offline",
                            "text": "Status is %status"
                        }
                    ]
                },
                "offlineEvents": {
                    "name": "Camera Offline events (24h)",
                    "display": "table|panel",
                    "calculate": "countValues %status 24h Offline",
                    "alarms": [
                        {
                            "level": "warning",
                            "condition": "greaterThan %offlineEvents 1",
                            "text": "went Offline %offlineEvents times in last 24 hours"
                        }
                    ]
                },
                "streamIssues1h": {
                    "name": "Stream issues (1h)",
                    "display": "table|panel",
                    "calculate": "sum %streamIssues 1h",
                    "insert": "streamIssues",
                    "alarms": [
                        {
                            "level": "warning",
                            "condition": "greaterThan %streamIssues1h 0",
                            "text": "has stream issues %streamIssues1h times in last hour"
                        },
                        {
                            "level": "error",
                            "condition": "greaterThan %streamIssues1h 5",
                            "text": "has stream issues %streamIssues1h times in last hour"
                        }
                    ]
                },
                "ipConflicts3min": {
                    "name": "IP conflicts (3min)",
                    "display": "panel",
                    "calculate": "sum %ipConflicts 3m",
                    "insert": "ipConflicts",
                    "alarms": [
                        {
                            "level": "error",
                            "condition": "greaterThan %ipConflicts3min 0",
                            "text": "has IP conflicts %ipConflicts3min times in last 3 minute"
                        }
                    ]
                }
            }
        },
        "primaryStream": {
            "values": {
                "resolution": {
                    "name": "Resolution",
                    "display": "table|panel"
                },
                "targetFps": {
                    "display": "Target fps"
                },
                "actualFps": {
                    "display": "Actual fps"
                },
                "fpsDeltaAvarage": {
                    "name": "Primary stream fps drops avarage (10m)",
                    "display": "panel",
                    "calculate": "average %fpsDelta 10m",
                    "alarms": [
                    {
                        "level": "warning",
                        "condition": "greaterThan %fpsDelta 5",
                        "text": "has actual fps less for %fpsDelta frames/s"
                    },
                    {
                        "level": "error",
                        "condition": "greaterThan %fpsDelta 10",
                        "text": "has actual fps less for %fpsDelta frames/s"
                    }
                    ]
                },
                "actualBitrateBps": {
                    "name": "Bitrate",
                    "display": "panel",
                    "format": "KBps",
                    "alarms": [
                        {
                            "level": "error",
                            "condition": "equal %actualBitrateBps 0",
                            "text": "Actual bitrate is 0 KBps"
                        }
                    ]
                }
            }
        },
        "secondaryStream": {
            "values": {
                "resolution": {
                    "name": "Resolution",
                    "display": "table|panel",
                    "alarms": [
                        {
                            "level": "error",
                            "condition": "resolutionGreaterThan %resolution 1280x720",
                            "text": "Resolution '%resolution' is too high"
                        }
                    ]
                },
                "targetFps": {
                    "display": "Target fps"
                },
                "actualFps": {
                    "display": "Actual fps"
                },
                "actualBitrateBps": {
                    "name": "Bitrate",
                    "display": "panel",
                    "format": "KBps",
                    "alarms": [
                        {
                            "level": "error",
                            "condition": "equal %actualBitrateBps 0",
                            "text": "Actual bitrate is 0 KBps"
                        }
                    ]
                },
                "fpsDeltaAvarage": {
                    "name": "Secondary stream fps drops avarage (10m)",
                    "display": "panel",
                    "calculate": "average %fpsDelta 10m"
                }
            }
        },
        "analytics": {
            "values": {
                "archiveLengthS": {
                    "name": "Archive length",
                    "display": "table|panel",
                    "format": "durationS"
                },
                "recordingBitrateBps": {
                    "name": "Recording Bitrate (24h)",
                    "display": "table|panel",
                    "format": "KBps"
                },
                "minArchiveLengthS": {
                    "name": "Minimum archive length",
                    "display": "panel",
                    "format": "durationS",
                    "alarms": [{
                        "level": "error",
                        "condition": "greaterThan %minArchiveLengthS %archiveLengthS",
                        "text": "Actual archive length %archiveLengthS is less than user defined minumal archive length %minArchiveLengthS"
                    }]
                }
            }
        }
    },
    "storages": {
        "info": {
            "values": {
                "name": {
                    "display": "table|panel"
                },
                "server": {
                    "display": "table|panel"
                },
                "type": {
                    "display": "table|panel"
                }
            }
        },
        "state": {
            "values": {
                "status": {
                    "display": "table|panel"
                },
                "issues24h": {
                    "name": "Issues (24h)",
                    "display": "table|panel",
                    "calculate": "sum %issues 24h",
                    "insert": "issues"
                }
            }
        },
        "activity": {
            "values": {
                "readRateBps1m": {
                    "name": "Read rate",
                    "display": "table|panel",
                    "format": "KBps",
                    "calculate": "average %readRateBps 1m",
                    "insert": "readRateBps"
                },
               "writeRateBps1m": {
                    "name": "Write rate",
                    "display": "table|panel",
                    "format": "KBps",
                    "calculate": "average %writeRateBps 1m",
                    "insert": "writeRateBps"
                },
               "transactionsPerSecond": {
                    "name": "Transactions/s",
                    "display": "panel",
                    "format": "Transactions/s"
                }
            }
        },
        "space": {
            "values": {
                "totalSpaceB": {
                    "name": "Total space",
                    "display": "table|panel",
                    "format": "GB"
                },
                "mediaSpaceP": {
                    "name": "VMS Media(%)",
                    "display": "table|panel",
                    "format": "%"
                },
                "mediaSpaceB": {
                    "name": "VMS Media",
                    "display": "panel",
                    "format": "GB"
                }
            }
        }
    },
    "networkInterfaces": {
        "info": {
            "values": {
                "name": {
                    "display": "table|panel"
                },
                "server": {
                    "display": "table|panel"
                },
                "ip": {
                    "name": "IP",
                    "display": "table|panel"
                }
            }
        },
        "rates": {
            "name": "I/O rates",
            "values": {
                "inBps": {
                    "name": "IN rate",
                    "format": "kbps",
                    "display": "table|panel"
                },
                "outBps": {
                    "name": "OUT rate",
                    "format": "kbps",
                    "display": "table|panel"
                }
            }
        }
    }
}
