{
    "__comment__": "                                                \
    Get width from here:                                            \
    http://en.wikipedia.org/wiki/Image_sensor_format                \
    Note: 1/2.8 is 5.2mm x 3.9mm, D=6.5mm.                          \
                                                                    \
    Calculate width-based crop factor C = 36/W.                     \
                                                                    \
    Multiply focal lengths by width-based crop factor to get        \
    width-based equivalent focal length.                            \
    ",
    
    "version": "1.0",
    "mappers": [
        {
            "models": [
                "AXIS Q6035-E"
            ],
            "fromCamera": {
                "x": {
                    "extrapolationMode": "PeriodicExtrapolation",
                    "logical":  [-1,   1],
                    "physical": [ 0, 360]
                },
                "y": {
                    "extrapolationMode": "ConstantExtrapolation",
                    "logical":  [-0.5, 0.1111111111111],
                    "physical": [ -90,              20]
                },
                "z": {
                    "extrapolationMode": "ConstantExtrapolation",
                    "logical":  [    0,     1],
                    "physical": [35.25, 711.1]
                }
            },
            "toCamera": {
                "y": {
                    "extrapolationMode": "ConstantExtrapolation",
                    "logical":  [-0.5, 0.0, 1.0],
                    "physical": [ -90, 0.0,  20]
                }
            }
        },
        {
            "models": [
                "DWC-MPTZ20X"
            ],
            "fromCamera": {
                "x": {
                    "extrapolationMode": "PeriodicExtrapolation",
                    "logical":  [-1,   1],
                    "physical": [ 0, 360]
                },
                "y": {
                    "extrapolationMode": "ConstantExtrapolation",
                    "logical":  [  0,    1],
                    "physical": [  0,  -90]
                },
                "z": {
                    "extrapolationMode": "ConstantExtrapolation",
                    "logical":  [0.004167, 0.083333],
                    "physical": [   35.25,    705.0]
                }
            }
        }
    ]
}






