<h1>{{event}} on <b>{{source}}</b></h1>

<table class="description">
<thead>
    <tr>
        <th>Reason</th>
        <th>Time</th>
    </tr>
</thead>
<tbody>
    {{#aggregated}}
        <tr>
            <td>{{reason}}</td>
            <td>{{{timestamp}}}</td>
        </tr>
    {{/aggregated}}
</tbody>
</table>