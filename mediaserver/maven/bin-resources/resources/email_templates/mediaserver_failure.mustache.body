<h1>Server <b>"{{source}}"</b> failed</h1>

<table class="description">
    <thead>
        <tr>
            <th>Reason</th>
            <th>Time</th>
        </tr>
    </thead>
    <tbody>
        {{#aggregated}}
            <tr>
                <td>{{reason}}</td>
                <td>{{{timestamp}}}</td>
            </tr>
        {{/aggregated}}
    </tbody>
</table>