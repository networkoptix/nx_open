<h1>{{event}}</h1>
<table class="description">
<tbody>
{{#aggregated}}
		<tr>
			{{#sourceIcon}}
			<td>
				<img src="{{sourceIcon}}">
			</td>
			{{/sourceIcon}}
			<td>
				<b class="ellipsis">{{source}}</b> <span class="ellipsis">({{sourceIp}})</span>
			</td>
			{{#aggregated}}
				<td>{{reason}}</td>
				<td><span class="ellipsis">{{timestampDate}}</span> <b class="ellipsis">{{timestampTime}}</b></td>
			{{/aggregated}}
		</tr>
{{/aggregated}}
</tbody>
</table>