<h1>{{event}}</h1>
<table class="description">
<tbody>
{{#aggregated}}
		<tr>
			{{#sourceIcon}}
			<td>
				<img src="{{sourceIcon}}">
			</td>
			{{/sourceIcon}}
			<td>
				<b class="source-name">{{source}}</b><br>
				({{sourceIp}})
			</td>
			{{#aggregated}}
				<td>{{reason}}</td>
				<td><span class="source-name">{{timestampDate}}</span><br><b>{{timestampTime}}</b></td>
			{{/aggregated}}
		</tr>
{{/aggregated}}
</tbody>
</table>