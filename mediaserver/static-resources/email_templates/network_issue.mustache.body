<h1>{{event}}</h1>
<table class="description">
<tbody>
{{#aggregated}}
		<tr>
			{{#sourceIcon}}
			<td>
				<img src="{{sourceIcon}}">
			</td>
			{{/sourceIcon}}
			<td>
				<b>{{source}}</b> ({{sourceIp}})
			</td>
			{{#aggregated}}
				<td class="error">{{reason}}</td>
				<td>{{timestampDate}} <b>{{timestampTime}}</b></td>
			{{/aggregated}}
		</tr>
{{/aggregated}}
</tbody>
</table>