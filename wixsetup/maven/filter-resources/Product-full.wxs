                <![CDATA[ (INSTALL_SERVER OR !ServerFeature=3) AND REMOVE <> "ALL" ]]></Custom>
            <Custom Action="CopyMediaServerProfile" After="CopyMediaServerProfile_Cmd">
                <![CDATA[ (INSTALL_SERVER OR !ServerFeature=3) AND REMOVE <> "ALL" ]]></Custom>

            <Custom Action="CertGenAction_Cmd" After="CopyAppServerProfile">
                <![CDATA[ $(var.InstallECS) AND REMOVE <> "ALL" ]]></Custom>
            <Custom Action="CertGenAction" After="CertGenAction_Cmd">
                <![CDATA[ $(var.InstallECS) AND REMOVE <> "ALL" ]]></Custom>

            <Custom Action="SaveLastDatabaseVersion_Cmd" After="CertGenAction">
                <![CDATA[ $(var.InstallECS) AND REMOVE <> "ALL" ]]></Custom>
            <Custom Action="SaveLastDatabaseVersion" After="SaveLastDatabaseVersion_Cmd">
                <![CDATA[ $(var.InstallECS) AND REMOVE <> "ALL" ]]></Custom>

            <Custom Action="BackupDatabaseFile_Cmd" After="SaveLastDatabaseVersion">
                <![CDATA[ $(var.InstallECS) AND REMOVE <> "ALL" ]]></Custom>
            <Custom Action="BackupDatabaseFile" After="BackupDatabaseFile_Cmd">
                <![CDATA[ $(var.InstallECS) AND REMOVE <> "ALL" ]]></Custom>

            <Custom Action="SyncDatabaseAction_Cmd_WithWarning" Before="SyncDatabaseAction">
                <![CDATA[ $(var.VmsUpgrading) AND $(var.InstallECS) AND REMOVE <> "ALL" ]]></Custom>
            <Custom Action="SyncDatabaseAction_Cmd_WithoutWarning" Before="SyncDatabaseAction">
                <![CDATA[ (NOT $(var.VmsUpgrading)) AND $(var.InstallECS) AND REMOVE <> "ALL" ]]></Custom>

            <Custom Action="SyncDatabaseAction" After="BackupDatabaseFile">
                <![CDATA[ $(var.InstallECS) AND REMOVE <> "ALL" ]]></Custom>

            <Custom Action="InitDatabaseAction_Cmd" After="SyncDatabaseAction">
                <![CDATA[ $(var.InstallECS) AND REMOVE <> "ALL" ]]>
            </Custom>
            <Custom Action="InitDatabaseAction" After="InitDatabaseAction_Cmd">
                <![CDATA[ $(var.InstallECS) AND REMOVE <> "ALL" ]]>
            </Custom>

            <Custom Action="DeleteDatabaseFile_Cmd" After="StopServices">REMOVE_DATABASE=1</Custom>
            <Custom Action="DeleteDatabaseFile" After="DeleteDatabaseFile_Cmd">REMOVE_DATABASE=1</Custom>
