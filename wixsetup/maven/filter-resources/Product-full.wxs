                            Win64="yes"/>
        </Property>

        <Property Id="SERVER_APPSERVER_HOST32" Secure="yes">
            <RegistrySearch Id="ServerEcsHostReg32"
                            Root="HKLM"
                            Key="Software\${company.name}\${mediaserver.name}"
