            <![CDATA[ ALLOW_CAMERA_CHANGES = "" ]]>
        </SetProperty>
        <SetProperty Action="SetALLOW_CAMERA_CHANGES_BEFORE_UI" Id="ALLOW_CAMERA_CHANGES" Value="{}" After="SetALLOW_CAMERA_CHANGES_DEFAULT" Sequence="ui">
