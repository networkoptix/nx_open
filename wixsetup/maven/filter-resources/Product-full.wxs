                            Name="appserverPassword"
                            Type="raw"
                            Win64="yes"/>
        </Property>

        <Property Id="SERVER_APPSERVER_PASSWORD32" Secure="yes">
            <RegistrySearch Id="ServerEcsPasswordReg32"
