<div class="content">
    <div class="callout">
        <h4>Information sur l'échec du Serveur Média:</h4>
        <table class="details">
            <tr>
                <th>Raison</th>
                <th>Temps</th>
            </tr>
            {{#aggregated}}
                <tr>
                    <td>{{reason}}</td>
                    <td><i>{{timestamp}}</i></td>
                </tr>
            {{/aggregated}}
        </table>
    </div>
</div>
