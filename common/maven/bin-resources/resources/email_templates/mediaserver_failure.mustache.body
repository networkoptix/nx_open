<h1>{{event}}: {{source}}</h1>


<table class="description">
    <thead>
        <tr>
            <th>#</th>
            <th>Reason</th>
            <th>Time</th>
        </tr>
    </thead>
    <tbody>
        {{#aggregated}}
            <tr>
                <td>{{number}}</td>
                <td>{{reason}}</td>
                <td>{{timestampDate}} <b>{{timestampTime}}</td>
            </tr>
        {{/aggregated}}
    </tbody>
</table>