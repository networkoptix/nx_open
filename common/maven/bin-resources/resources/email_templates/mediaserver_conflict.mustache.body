{{#aggregated}}
<div class="content">
    <div class="callout">
        <p>Conflict detected at: {{timestamp}}</p>
        <table class="details">
            <tr>
                <th>Number</th>
                <th>ECS IP</th>
                <th>Camera Physical ID</th>
            </tr>
                {{#msConflicts}}
                    <tr>
                        <td>{{number}}</td>
                        <td>{{ip}}</td>
                        <td>{{mac}}</td>
                    </tr>
                {{/msConflicts}}
        </table>
    </div>
</div>
{{/aggregated}}
