<h1>{{event}} on {{source}}</h1>

<table class="description">
<thead>
    <tr>
        <th>Reason</th>
        <th>Time</th>
    </tr>
</thead>
<tbody>
    {{#aggregated}}
        <tr>
            <td class="error">{{reason}}</td>
            <td>{{timestampDate}} <b>{{timestampTime}}</td>
        </tr>
    {{/aggregated}}
</tbody>
</table>