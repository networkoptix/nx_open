<h1>{{event}}: {{source}}</h1>

<table class="description">
<thead>
    <tr>
        <th>Reason</th>
        <th>Time</th>
    </tr>
</thead>
<tbody>
    {{#aggregated}}
        <tr>
            <td class="error">{{reason}}</td>
            <td><i>{{timestamp}}</i></td>
        </tr>
    {{/aggregated}}
</tbody>
</table>