<h1>{{event}}</h1>
<table class="description">
<tbody>
{{#aggregated}}
		<tr>
			<td>
				<img src="{{sourceIcon}}">
			</td>
			<td>
				<b>{{sourceName}}</b><br>
				({{sourceIp}})
			</td>
			{{#aggregated}}
				<td class="error">{{reason}}</td>
				<td>{{timestampDate}}<br><b>{{timestampTime}}</b></td>
			{{/aggregated}}
		</tr>
{{/aggregated}}
</tbody>
</table>