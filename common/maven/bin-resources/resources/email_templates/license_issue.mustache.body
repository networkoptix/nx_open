<h1>{{event}}: {{source}}</h1>
<p>Camera recording was forcibly stopped because of not enough license for recording</p>
<table class="description">
    <thead>
    <tr>
        <th>Cameras</th>
        <th>Time</th>
    </tr>
    </thead>
    <tbody>
    {{#aggregated}}
        <tr>
            <td>{{reason}}</td>
            <td><i>{{timestamp}}</i></td>
        </tr>
    {{/aggregated}}
    </tbody>
</table>
