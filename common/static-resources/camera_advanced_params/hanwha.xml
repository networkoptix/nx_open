            <param id="image/flip/Rotate"
                    name="Hallway View"
                    dataType="Enumeration"
                    aux="streamsToReopen=primary,secondary"/>
        </group>
        <group name="Audio Input">
            <param id="media/audioinput/Source"
                    name="Source"
                    dataType="Enumeration"
                    range=""
                    aux="support=Media/Limit/MaxAudioInput"/>
            <param id="media/audioinput/EncodingType"
                    name="Codec"
                    dataType="Enumeration"
                    aux="support=Media/Limit/MaxAudioInput;range=media/audioinput/set/EncodingType"/>
            <param id="media/audioinput/Bitrate"
                    name="Bitrate"
                    dataType="Enumeration"
                    range="16000,24000,32000,40000,64000"
                    aux="support=Media/Limit/MaxAudioInput">
                <dependencies>
                    <dependencies-ranges>
                        <conditional-range id="g711Range" range="64000">
                            <condition
                                watch="media/audioinput/EncodingType"
                                condition="value=G711"/>
                        </conditional-range>
                        <conditional-range id="g726Range" range="16000,24000,32000,40000,64000">
                            <condition
                                watch="media/audioinput/EncodingType"
                                condition="value=G726"/>
                        </conditional-range>
                        <conditional-range id="aacRange" range="48000">
                            <condition
                                watch="media/audioinput/EncodingType"
                                condition="value=AAC"/>
                        </conditional-range>
                    </dependencies-ranges>
                </dependencies>
            </param>
            <param id="media/audioinput/Gain"
                    name="Gain"
                    dataType="Number"
                    range="1,10"
                    aux="support=Media/Limit/MaxAudioInput"/>
        </group>

