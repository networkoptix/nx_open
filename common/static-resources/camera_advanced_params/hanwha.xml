                <param id="image/camera/SSNRMode"
                        name="SSNR Mode"
                        dataType="Enumeration"/>
                <param id="image/camera/SSNRLevel"
                        name="SSNR Level"
                        dataType="Number">
                    <dependencies>
                        <dependencies-show>
                            <conditional-show>
                                <condition 
                                    watch="image/camera/SSNRMode"
                                    condition="value=Manual"/>
                            </conditional-show>
                        </dependencies-show>
                    </dependencies>
                </param>
                <param id="image/camera/AGCMode"
                        name="AGC"
                        dataType="Enumeration"
                        range="Off,Low,Middle,High"/>
            </group>
            <group name="Day/Night">
                <param id="image/camera/DayNightMode"
                        name="Mode"
                        dataType="Enumeration"
                        range="Color,B/W,Auto,External"
                        internalRange="Color,BW,Auto,ExternalBW"/>
                <!--<param id="root.ImageSource.I0.Sensor.ColorLevel"
                        name="Dwell Time"
                        dataType="Enumeration"/>-->
                <param id="image/camera/DayNightSwitchingMode"
                        name="Duration"
                        dataType="Enumeration"
                        range="Very fast,Fast,Normal,Slow,Very slow"/>
                <param id="image/camera/DayNightAlarmIn"
                        name="Alarm Input"
                        dataType="Enumeration"
                        range="Open:Color/Close:BW,Open:BW/Close:Color"
                        internalRange="SwitchToBWIfCloses,SwitchToBWIfOpens">
                    <dependencies>
                        <dependencies-show>
                            <conditional-show>
                                <condition
                                    watch="image/camera/DayNightMode"
                                    condition="value=External"/>
                            </conditional-show>
                        </dependencies-show>
                    </dependencies>
                </param>
            </group>
            <group name="Special">
                <param id="image/imageenhancements/SharpnessEnable"
                        name="Sharpness"
                        dataType="Bool"/>
                <param id="image/imageenhancements/SharpnessLevel"
                        name="Sharpness Level"
                        dataType="Number"/>
                <param id="image/imageenhacements/Gamma"
                        name="Gamma"
                        dataType="Number"/>
                <param id="image/imageenhacements/Saturation"
                        name="ColorLevel"
                        dataType="Number"/>
                <param id="image/imageenhacements/LDCEnable" 
                        name="LDC"
                        dataType="Bool"/>
                <param id="image/imageenhacements/LDCLevel"
                        name="LDC Level"
                        dataType="Number"/>
            </group>
            <group name="IR">
                <param id="image/irled/Mode"
                        name="Mode"
                        dataType="Enumeration"/>
            </group>
        </group>
        <group name="Maintenance">
            <param id="system/factoryreset/ExcludeSettings=Network,Authority"
                        name="Soft Reset"
                        dataType="Button"
                        writeCmd="control"
                        aux="noChannel"/>
            <param id="system/factoryreset/ExludeSettings=None"
                        name="Hard Reset"
                        dataType="Button"
                        writeCmd="control"
                        aux="noChannel"/>
            <param id="system/power/Type=Restart"
                        name="Reboot"
                        dataType="Button"
                        writeCmd="control"
                        aux="noChannel"/>
        </group>
    </parameters>
</plugin>
