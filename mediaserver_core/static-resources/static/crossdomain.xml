<?xml version="1.0"?>
<!DOCTYPE cross-domain-policy SYSTEM "http://www.macromedia.com/xml/dtds/cross-domain-policy.dtd">
<cross-domain-policy>
    <allow-access-from domain="%SERVER_IF_LIST%" to-ports="*" />
    <allow-access-from domain="*.%CLOUD_PORTAL_URL%" to-ports="*" />
    <site-control permitted-cross-domain-policies="all"/>
</cross-domain-policy>
