    <groupId>com.networkoptix.hdwitness</groupId>
    <version>3.2.0-SNAPSHOT</version>
    <modelVersion>4.0.0</modelVersion>
    <artifactId>cloud_connect_auto_test</artifactId>
    <packaging>pom</packaging>
    <name>Nx Cloud Connect Auto Test</name>

    <properties>
        <guid>${cloud_connect_auto_test.guid}</guid>
        <product.title>${company.name} Cloud Connect Auto Test</product.title>

        <qt.libs>core concurrent xml</qt.libs>
        <global.libs>-lcommon -lnx_network -ludt -lnx_fusion -lnx_utils</global.libs>
        <windows.oslibs>-lws2_32 -lIphlpapi DbgHelp.lib -lwinmm</windows.oslibs>
        <mac.oslibs>-lbz2 -framework IOKit -framework CoreServices</mac.oslibs>

        <rdep.packages>openssl quazip</rdep.packages>
    </properties>

    <dependencies>
        <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>nx_network</artifactId>
            <version>${project.version}</version>
            <type>pom</type>
            <scope>system</scope>
            <systemPath>${root.dir}/common_libs/nx_network/pom.xml</systemPath>
        </dependency>
    </dependencies>

</project>
