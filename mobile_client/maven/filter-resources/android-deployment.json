{
   "description": "Configuration file for androiddeployqt.",
   "qt": "${libdir}",
   "sdk": "${env.environment}/android/android-sdk",
   "ndk": "${env.environment}/android/android-ndk",
   "toolchain-prefix": "arm-linux-androideabi",
   "tool-prefix": "arm-linux-androideabi",
   "toolchain-version": "4.8",
   "ndk-host": "linux-x86_64",
   "target-architecture": "armeabi-v7a",
   "android-extra-libs": "${libdir}/lib/${build.configuration}/libssl.so,${libdir}/lib/${build.configuration}/libquazip.so,${libdir}/lib/${build.configuration}/libcrypto.so",
   "application-binary": "${libdir}/lib/${build.configuration}/libmobile_client.so"
}
